<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
  <fullName>Contact.Contact_required_fields</fullName>
  <active>true</active>
  <errorConditionFormula>AND(
  ISBLANK( Email ),
  ISBLANK( MobilePhone )
)</errorConditionFormula>
  <errorMessage>Email and Phone cannot be null.</errorMessage>
</ValidationRule>